{
  "name": "react-native-reader",
  "version": "0.4.7",
  "description": "React Native package that turns a Webview into a Safari like Readability View for android and iOS",
  "keywords": [
    "react-native-readability",
    "react-native-reader",
    "react-native",
    "readability",
    "reader",
    "ios-reader",
    "android-reader",
    "safari-reader"
  ],
  "main": "dist/reader.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffreymendez1993/react-native-reader.git"
  },
  "scripts": {
    "test": "jest -u",
    "prepare": "tsc"
  },
  "author": "Jeffrey Mendez <jeffmendez19@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffreymendez1993/react-native-reader/issues"
  },
  "homepage": "https://github.com/jeffreymendez1993/react-native-reader#readme",
  "dependencies": {
    "clean-html-js": "^1.2.0",
    "react-native-htmlview": "^0.16.0"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.0.2",
    "@types/jest": "^26.0.21",
    "@types/react": "^16.9.50",
    "@types/react-native": "^0.63.23",
    "jest": "^26.4.2",
    "jest-fetch-mock": "^3.0.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "^0.63.0",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "xhr-mock": "^2.5.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  }
}
